<%= form_for @subsector, method: :post, class: "navbar-form navbar-left search-form" do |f| %>
  <label>Name:</label>
    <%= f.text_field :name, size: 40 -%>
  <br />
  <label>Description:</label>
    <%= f.text_area :description, cols: 60, rows: 10 -%>
  <br/>
  <label>UWP:</label>
    <%= f.text_area :uwp, cols: 60, rows: 20 -%>

  <%= f.submit('Create Subsector') %>
<% end %>
